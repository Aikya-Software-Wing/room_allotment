@model IEnumerable<ExamRoomAllocation.Models.TeacherRoom>

@{
    ViewBag.Title = "Export";
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Teachers</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>TeacherId</th>
            <th>RoomId</th>
            <th>SessionId</th>

        </tr>
        
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Teacher_Id</td>
                <td></td>
                <td>@item.Room_Id</td>
                <td>@item.Session_Id</td>
            </tr>
        }
    </table>
    <br />
    <br />
    @using (Html.BeginForm("Export", "Download", FormMethod.Post))
    {
        <input type="submit" value="Export" />
    }
</body>
</html>

